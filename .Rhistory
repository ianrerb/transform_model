f<-function(S,u,r,q,t,k,th,rho,sigma,v0){
i = complex(0,1);
gamma = sqrt(sigma^2*(u^2+i*u)+(k-i*rho*sigma*u)^2);
result = exp(i*u*log(S)+i*(r-q)*t*u+(k*th*t*(k-i*rho*sigma*u))/(sigma*sigma));
result = result/((cosh(gamma*t/2)+(k-i*rho*sigma*u)/gamma*sinh(gamma*t/2))^(2*k*theta/(sigma*sigma)));
result = result*exp(-(u^2*v0+i*u*v0)/(gamma*cosh(gamma*t/2)/sinh(gamma*t/2)+k-i*rho*sigma*u));
return(result);
}
